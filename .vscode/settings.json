{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#b8a4e3",
    "activityBar.background": "#b8a4e3",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#ab5a35",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#b8a4e3",
    "statusBar.background": "#9a7dd7",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#7c56cb",
    "statusBarItem.remoteBackground": "#9a7dd7",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#9a7dd7",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#9a7dd799",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "explorer.sortOrder": "default",
  "peacock.color": "#9a7dd7",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "eslint.useFlatConfig": true,
  "prettier.configPath": ".prettierrc",
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "scss.lint.unknownAtRules": "ignore",
  "eslint.format.enable": true,
  "prettier.useEditorConfig": false,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "i18n-ally.localesPaths": ["locales"],
  "mdc.enableFormatting": true,
  "[mdc]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.formatOnPaste": true
  },
  "[md]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.formatOnPaste": true
  }
}
