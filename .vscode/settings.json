{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#7c7ad4",
    "activityBar.background": "#7c7ad4",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#91312f",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#7c7ad4",
    "statusBar.background": "#5653c8",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#7c7ad4",
    "statusBarItem.remoteBackground": "#5653c8",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#5653c8",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#5653c899",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "explorer.sortOrder": "default",
  "peacock.color": "#5653c8",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "eslint.useFlatConfig": true,
  "prettier.configPath": ".prettierrc",
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "scss.lint.unknownAtRules": "ignore",
  "eslint.format.enable": true,
  "prettier.useEditorConfig": false,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "i18n-ally.localesPaths": [],
  "mdc.enableFormatting": true,
  "[mdc]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.formatOnPaste": true
  },
  "[md]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.formatOnPaste": true
  }
}
